<h3>Edit <%= current_user.email %></h3>
<%= render "layouts/errors", :object => current_user %>

  <%= form_for @user do |f| %>
  <%= f.label :user_name %>
  <%= f.text_field :user_name %>
  <%= f.label :email  %>
  <%= f.text_field :email %>
  <%= f.check_box :admin, :value => true %>
  <%= f.label :admin %>
  <br>
  <%= submit_tag 'Edit Profile' %>
<% end %>
